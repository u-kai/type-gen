{
  "src": {
    "root": "examples/jsons",
    "extension": "json"
  },
  "dist": {
    "root": "examples/dist",
    "extension": "rs"
  },
  "comment": {
    "alltype": "this is auto make type",
    "allproperty": "this is auto make property"
  },
  "attribute": {
